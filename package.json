    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.33.1",
    "swagger-ui-react": "^4.1.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/cookie": "^0.4.1",
    "@types/jest": "^27.0.3",
    "@types/node": "16.11.7",
    "@types/react": "17.0.35",
