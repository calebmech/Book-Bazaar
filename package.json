    "eslint-config-next": "12.0.4",
    "isarray": "^2.0.5",
    "jest": "^27.3.1",
    "local-ssl-proxy": "1.2.3",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.5.0",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.0.7",
    "typescript": "4.5.2"
  }
